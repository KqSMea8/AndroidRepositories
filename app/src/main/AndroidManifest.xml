<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lenovo.androidrepositories">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.GifViewTestActivity"
            android:label="GifView">

        </activity>
        <activity
            android:name=".view.activity.DrawBoardTestActivity"
            android:label="画板View">

        </activity>
        <activity
            android:name=".view.activity.DragLayoutTestActivity"
            android:label="抽屉">

        </activity>
        <activity
            android:name=".view.activity.AppTestActivity"
            android:label="应用">

        </activity>
        <activity
            android:name=".view.activity.FolderTestActivity"
            android:label="文件">

        </activity>
        <activity
            android:name=".view.activity.ZipTestActivity"
            android:label="Zip"></activity>
        <activity
            android:name=".view.activity.SlideBackTestActivity"
            android:label="SlideBackLayout"
            android:theme="@style/transparent_theme"></activity>
        <activity
            android:name=".view.activity.DirectionalViewPagerTestActivity"
            android:label="垂直ViewPager"></activity>
        <activity
            android:name=".view.activity.CustomViewPagerTestActivity"
            android:label="自定义ViewPager"></activity>
        <activity
            android:name=".view.activity.CustomAnimatorActivity"
            android:label="自定义动画"></activity>
        <activity
            android:name=".view.activity.ThreadTestActivity"
            android:label="多线程练习"></activity>
    </application>

</manifest>